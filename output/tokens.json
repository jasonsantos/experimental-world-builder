  "1 - brands": {
    "$collection": {
      "$modes": [
        "Photostop",
        "Saddles"
      ]
    },
    "brand": {
      "neutral": {
        "$type": "color",
        "$value": {
          "Saddles": {
            "$alias": "0 - primitives.color.black",
            "$mode": "Mode 1"
          },
          "Photostop": {
            "$alias": "0 - primitives.color.black",
            "$mode": "Mode 1"
          }
        },
        "$extensions": {
          "com.specifyapp.figma-variable.scopes": [
            "ALL_SCOPES"
          ]
        }
      },
      "primary": {
        "$type": "color",
        "$value": {
          "Saddles": {
            "$alias": "0 - primitives.color.amber.500",
            "$mode": "Mode 1"
          },
          "Photostop": {
            "$alias": "0 - primitives.color.red.500",
            "$mode": "Mode 1"
          }
        },
        "$extensions": {
          "com.specifyapp.figma-variable.scopes": [
            "ALL_SCOPES"
          ]
        }
      },
      "secondary": {
        "$type": "color",
        "$value": {
          "Saddles": {
            "$alias": "0 - primitives.color.black",
            "$mode": "Mode 1"
          },
          "Photostop": {
            "$alias": "0 - primitives.color.white",
            "$mode": "Mode 1"
          }
        },
        "$extensions": {
          "com.specifyapp.figma-variable.scopes": [
            "ALL_SCOPES"
          ]
        }
      }
    }
  },
  "2 - modes": {
    "$collection": {
      "$modes": [
        "light",
        "dark"
      ]
    },
    "mode": {
      "color": {
        "surface": {
          "background": {
            "$type": "color",
            "$value": {
              "dark": {
                "$alias": "1 - brands.brand.secondary",
                "$mode": "Saddles"
              },
              "light": {
                "$alias": "1 - brands.brand.primary",
                "$mode": "Photostop"
              }
            },
            "$extensions": {
              "com.specifyapp.figma-variable.scopes": [
                "ALL_SCOPES"
              ]
            }
          },
          "foreground": {
            "$type": "color",
            "$value": {
              "dark": {
                "$alias": "1 - brands.brand.primary",
                "$mode": "Saddles"
              },
              "light": {
                "$alias": "1 - brands.brand.secondary",
                "$mode": "Photostop"
              }
            },
            "$extensions": {
              "com.specifyapp.figma-variable.scopes": [
                "ALL_SCOPES"
              ]
            }
          }
        },
        "text": {
          "active": {
            "$type": "color",
            "$value": {
              "dark": {
                "$alias": "1 - brands.brand.secondary",
                "$mode": "Saddles"
              },
              "light": {
                "$alias": "1 - brands.brand.neutral",
                "$mode": "Photostop"
              }
            },
            "$extensions": {
              "com.specifyapp.figma-variable.scopes": [
                "ALL_SCOPES"
              ]
            }
          },
          "default": {
            "$type": "color",
            "$value": {
              "dark": {
                "$alias": "1 - brands.brand.primary",
                "$mode": "Saddles"
              },
              "light": {
                "$alias": "1 - brands.brand.neutral",
                "$mode": "Photostop"
              }
            },
            "$extensions": {
              "com.specifyapp.figma-variable.scopes": [
                "ALL_SCOPES"
              ]
            }
          }
        }
      }
    }
  },
  "3 - breakpoints": {
    "$collection": {
      "$modes": [
        "mobile",
        "desktop"
      ]
    },
    "breakpoint": {
      "base": {
        "unit": {
          "$type": "dimension",
          "$value": {
            "mobile": {
              "$alias": "0 - primitives.spacing.sm",
              "$mode": "Mode 1"
            },
            "desktop": {
              "$alias": "0 - primitives.spacing.md",
              "$mode": "Mode 1"
            }
          },
          "$extensions": {
            "com.specifyapp.figma-variable.scopes": [
              "ALL_SCOPES"
            ]
          }
        }
      },
      "margins": {
        "lg": {
          "$type": "dimension",
          "$value": {
            "mobile": {
              "$alias": "0 - primitives.spacing.md",
              "$mode": "Mode 1"
            },
            "desktop": {
              "$alias": "0 - primitives.spacing.lg",
              "$mode": "Mode 1"
            }
          },
          "$extensions": {
            "com.specifyapp.figma-variable.scopes": [
              "ALL_SCOPES"
            ]
