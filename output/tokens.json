{
  "0 - primitives": {
    "$collection": {
      "$modes": [
        "Mode 1"
      ]
    },
    "color": {
      "amber": {
        "500": {
          "$type": "color",
          "$value": {
            "Mode 1": {
              "model": "rgb",
              "red": 197,
              "green": 153,
              "blue": 0,
              "alpha": 1
            }
          },
          "$extensions": {
            "com.specifyapp.figma-variable.scopes": [
              "ALL_SCOPES"
            ]
          }
        }
      },
      "black": {
        "$type": "color",
        "$value": {
          "Mode 1": {
            "model": "rgb",
            "red": 0,
            "green": 0,
            "blue": 0,
            "alpha": 1
          }
        },
        "$extensions": {
          "com.specifyapp.figma-variable.scopes": [
            "ALL_SCOPES"
          ]
        }
      },
      "red": {
        "500": {
          "$type": "color",
          "$value": {
            "Mode 1": {
              "model": "rgb",
              "red": 255,
              "green": 0,
              "blue": 0,
              "alpha": 1
            }
          },
          "$extensions": {
            "com.specifyapp.figma-variable.scopes": [
              "ALL_SCOPES"
            ]
          }
        }
      },
      "white": {
