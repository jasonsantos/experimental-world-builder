        "$value": {
          "Mode 1": {
            "model": "rgb",
            "red": 255,
            "green": 255,
            "blue": 255,
            "alpha": 1
          }
        },
        "$extensions": {
          "com.specifyapp.figma-variable.scopes": [
            "ALL_SCOPES"
          ]
        }
      }
    },
    "spacing": {
      "lg": {
        "$type": "dimension",
        "$value": {
          "Mode 1": {
            "value": 8,
            "unit": "px"
          }
        },
        "$extensions": {
          "com.specifyapp.figma-variable.scopes": [
            "ALL_SCOPES"
          ]
        }
      },
      "md": {
        "$type": "dimension",
